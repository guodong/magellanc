struct Packet {
  ip_t src_ip
  ip_t dst_ip
  port_t src_port
  port_t dst_port
  int8_t protocol
}


int onPacket(struct Packet p) {
  int egress = 0
  if (p.src_ip == 167772161) {
     if (p.dst_ip == 167772162) {
       if (p.dst_port < 1025) {
         egress = 1
        }
     }
  }
  return egress
}

